# Google ADK configuration
GOOGLE_GENAI_USE_VERTEXAI=True
GOOGLE_CLOUD_PROJECT="your-gcp-project-id"
GOOGLE_CLOUD_LOCATION="us-central1"

# Required when deploying Google ADK agent to Vertex AI Agent Engine
GOOGLE_CLOUD_STORAGE_BUCKET="your-storage-bucket"

# URL of your deployed OAuth Proxy (local or Railway)
# For Railway: https://your-app-name.railway.app
OAUTH_PROXY_URL=http://localhost:5000         

# API key for programmatic access to OAuth Proxy
# Must match OAUTH_PROXY_API_KEY in your OAuth Proxy's settings
OAUTH_PROXY_API_KEY=your_secure_api_key_here
